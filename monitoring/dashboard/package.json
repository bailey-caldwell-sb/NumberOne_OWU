{
  "name": "numberone-owu-dashboard",
  "version": "1.0.0",
  "description": "System monitoring dashboard for NumberOne OWU",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "eslint .",
    "build": "npm run build:css && npm run build:js",
    "build:css": "tailwindcss -i ./src/input.css -o ./public/css/style.css --minify",
    "build:js": "webpack --mode production"
  },
  "keywords": [
    "ai",
    "monitoring",
    "dashboard",
    "ollama",
    "openwebui",
    "langfuse"
  ],
  "author": "NumberOne OWU Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.0",
    "ws": "^8.14.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "systeminformation": "^5.21.20"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "eslint": "^8.54.0",
    "tailwindcss": "^3.3.6",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
